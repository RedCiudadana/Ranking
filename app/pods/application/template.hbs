<EmberTable as |t|>
  <t.head @columns={{this.columns}} />
  <t.body @rows={{model}} as |b|>
    <b.row as |r|>
      <r.cell as |value column row cellMeta columnMeta rowMeta|>
        {{#if (is-equal column.name "Nivel Cumplimiento")}}
          <ScoreColor @score={{value}}/>
        {{else}}
          <AreaGraph @score={{value}} @maxScore={{30}} />
        {{/if}}
      </r.cell>
    </b.row>
  </t.body>
</EmberTable>

<style>
  td {
    padding: 3px;
  }
</style>